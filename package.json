{
  "name": "site-blocker",
  "version": "1.0.0",
  "description": "Site Blocker Extensino",
  "scripts": {
    "start": "npm run sass:build && snowpack dev",
    "build": "npm run sass:build:prod && snowpack build",
    "dev": "npm run sass:build:prod && npm run build && web-ext run -s build",
    "sass:build": "node scripts/buildSass.js",
    "sass:build:prod": "npm run sass:build && npm run sass:copy",
    "sass:copy": "node scripts/copyDistToBuild.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "fs-extra": "^9.1.0",
    "sass": "^1.32.12",
    "snowpack": "^3.3.7",
    "web-ext": "^6.1.0"
  },
  "dependencies": {}
}
